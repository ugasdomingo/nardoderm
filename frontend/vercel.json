{
    "version": 2,
    "builds": [
        {
            "src": "src/assets/**",
            "use": "@vercel/static"
        },
        {
            "src": "vite.config.js",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/src/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}
