{
    "version": 2,
    "builds": [
        {
            "src": "vite.config.js",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1",
            "rewrite": {
                "source": "/@/(.*)",
                "destination": "/src/$1"
            }
        }
    ]
}
